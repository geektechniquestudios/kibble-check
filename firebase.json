{
  "firestore": {
    "target": "has-the-dog-been-fed",
    "rules": "firestore.rules"
  },
  "hosting": [
    {
      "target": "has-the-dog-been-fed",
      "predeploy": "yarn build",
      "public": "dist",
      "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  }
}
